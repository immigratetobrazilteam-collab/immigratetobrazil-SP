{
  "version": "3.0.0",
  "state": "sp",
  "components": {
    "button": {
      "base": "px-6 py-3 rounded-lg font-semibold transition-all duration-300 inline-flex items-center gap-2",
      "variants": {
        "primary": "bg-[#3a4660] text-[#ffffff] hover:bg-[#2e384c] active:scale-95 shadow-md hover:shadow-lg",
        "secondary": "bg-[#c9af98] text-[#000000] hover:bg-[#a08c79] active:scale-95",
        "accent": "bg-[#845007] text-[#ffffff] hover:bg-[#694005] transform hover:scale-105 active:scale-95",
        "ghost": "bg-transparent text-[#3a4660] hover:bg-[#212121]",
        "outline": "bg-transparent text-[#3a4660] border-2 border-[#3a4660] hover:bg-[#455473]",
        "link": "bg-transparent text-[#3a4660] hover:text-[#2e384c] underline"
      },
      "sizes": {
        "xs": "px-2 py-1 text-xs",
        "sm": "px-4 py-2 text-sm",
        "md": "px-6 py-3 text-base",
        "lg": "px-8 py-4 text-lg",
        "xl": "px-10 py-5 text-xl"
      },
      "states": {
        "loading": "opacity-75 cursor-wait",
        "disabled": "opacity-50 cursor-not-allowed pointer-events-none"
      }
    },
    "card": {
      "base": "rounded-xl overflow-hidden transition-all duration-300",
      "variants": {
        "default": "bg-[#212121] border border-[#ffffff] shadow-md hover:shadow-lg",
        "elevated": "bg-[#212121] shadow-lg hover:shadow-2xl transform hover:-translate-y-1",
        "glass": "bg-white/10 backdrop-blur-lg border border-white/20 shadow-xl",
        "premium": "bg-gradient-to-br from-[#3a4660] to-[#845007] shadow-premium text-white",
        "interactive": "bg-[#212121] hover:shadow-xl hover:-translate-y-2 cursor-pointer active:scale-98"
      },
      "padding": {
        "none": "p-0",
        "sm": "p-4",
        "md": "p-6",
        "lg": "p-8",
        "xl": "p-10"
      }
    },
    "input": {
      "base": "w-full px-4 py-3 rounded-lg border bg-[#202020] text-[#d8d8d8] transition-all duration-200 placeholder:text-[#f0f0f0]",
      "states": {
        "default": "border-[#f0f0f0]",
        "focus": "border-[#3a4660] ring-4 ring-[#3a4660]/20 outline-none scale-101",
        "error": "border-[#ff414e] ring-4 ring-[#ff414e]/20",
        "success": "border-[#68d388] ring-4 ring-[#68d388]/20",
        "disabled": "bg-[#ffffff] text-[#f0f0f0] cursor-not-allowed opacity-50"
      },
      "sizes": {
        "sm": "px-3 py-2 text-sm",
        "md": "px-4 py-3 text-base",
        "lg": "px-5 py-4 text-lg"
      }
    },
    "breadcrumb": {
      "container": "flex items-center gap-2 text-sm",
      "item": "text-[#d8d8d8] hover:text-[#3a4660] transition-colors",
      "separator": "text-[#f0f0f0]",
      "active": "text-[#3a4660] font-semibold"
    },
    "tabs": {
      "container": "border-b border-[#ffffff]",
      "list": "flex gap-4",
      "tab": "px-6 py-3 text-[#d8d8d8] hover:text-[#3a4660] border-b-2 border-transparent transition-all",
      "activeTab": "text-[#3a4660] border-[#3a4660] font-semibold",
      "panel": "py-6 animate-fadeIn"
    },
    "accordion": {
      "container": "border border-[#ffffff] rounded-lg overflow-hidden",
      "item": "border-b border-[#ffffff] last:border-0",
      "header": "px-6 py-4 bg-[#212121] hover:bg-[#1f1f1f] cursor-pointer flex justify-between items-center",
      "panel": "px-6 py-4 animate-slideDown",
      "icon": "transition-transform duration-300"
    },
    "tooltip": {
      "base": "absolute bg-[#d8d8d8] text-white text-sm px-3 py-2 rounded-md shadow-lg pointer-events-none z-50",
      "arrow": "absolute w-2 h-2 bg-[#d8d8d8] transform rotate-45"
    },
    "popover": {
      "base": "absolute bg-[#212121] border border-[#ffffff] rounded-xl shadow-xl p-4 z-50",
      "arrow": "absolute w-3 h-3 bg-[#212121] border border-[#ffffff] transform rotate-45"
    },
    "rating": {
      "container": "inline-flex gap-1",
      "star": "cursor-pointer transition-all duration-200 hover:scale-110",
      "filled": "text-[#fbbf24]",
      "empty": "text-[#ffffff]"
    },
    "badge": {
      "base": "inline-flex items-center px-3 py-1 rounded-full text-sm font-medium",
      "variants": {
        "default": "bg-[#ffffff] text-[#d8d8d8]",
        "primary": "bg-[#3a4660] text-[#ffffff]",
        "success": "bg-[#68d388] text-white",
        "warning": "bg-[#ffb766] text-black",
        "error": "bg-[#ff414e] text-white"
      },
      "dot": "absolute -top-1 -right-1 w-3 h-3 rounded-full"
    },
    "avatar": {
      "base": "rounded-full overflow-hidden flex items-center justify-center",
      "sizes": {
        "xs": "w-6 h-6 text-xs",
        "sm": "w-8 h-8 text-sm",
        "md": "w-10 h-10 text-base",
        "lg": "w-12 h-12 text-lg",
        "xl": "w-16 h-16 text-xl",
        "2xl": "w-20 h-20 text-2xl"
      },
      "group": "flex -space-x-2"
    },
    "pagination": {
      "container": "flex items-center gap-2",
      "button": "px-4 py-2 border border-[#ffffff] rounded-lg hover:bg-[#455473] transition-colors",
      "active": "bg-[#3a4660] text-[#ffffff] border-[#3a4660]",
      "disabled": "opacity-50 cursor-not-allowed"
    },
    "modal": {
      "backdrop": "fixed inset-0 bg-black/50 backdrop-blur-sm z-40",
      "container": "fixed top-1/2 left-1/2 -translate-x-1/2 -translate-y-1/2 bg-[#212121] rounded-2xl shadow-2xl max-w-2xl w-full z-50",
      "header": "px-6 py-4 border-b border-[#ffffff] flex justify-between items-center",
      "body": "px-6 py-4",
      "footer": "px-6 py-4 border-t border-[#ffffff] flex justify-end gap-2"
    },
    "drawer": {
      "backdrop": "fixed inset-0 bg-black/50 z-40",
      "container": "fixed top-0 h-full bg-[#212121] shadow-2xl z-50 w-80",
      "left": "left-0",
      "right": "right-0"
    },
    "notification": {
      "base": "flex items-start gap-3 bg-[#212121] border rounded-lg shadow-lg p-4 max-w-md",
      "info": "border-[#51d0de] bg-[#84e0e9]/10",
      "success": "border-[#68d388] bg-[#a3e5b5]/10",
      "warning": "border-[#ffb766] bg-[#ffd099]/10",
      "error": "border-[#ff414e] bg-[#ff7481]/10"
    },
    "progressBar": {
      "container": "w-full bg-[#ffffff] rounded-full h-2 overflow-hidden",
      "fill": "h-full bg-[#3a4660] transition-all duration-300",
      "striped": "bg-gradient-to-r from-transparent via-white/20 to-transparent bg-[length:30px_100%] animate-[shimmer_2s_infinite]"
    },
    "spinner": {
      "base": "animate-spin rounded-full border-2 border-transparent",
      "primary": "border-t-[#3a4660]",
      "sizes": {
        "sm": "w-4 h-4",
        "md": "w-8 h-8",
        "lg": "w-12 h-12",
        "xl": "w-16 h-16"
      }
    },
    "carousel": {
      "container": "relative overflow-hidden",
      "track": "flex transition-transform duration-500 ease-out",
      "slide": "flex-shrink-0 w-full",
      "control": "absolute top-1/2 -translate-y-1/2 bg-black/50 hover:bg-black/80 text-white p-3 rounded-full transition-colors",
      "dots": "absolute bottom-4 left-1/2 -translate-x-1/2 flex gap-2"
    },
    "navigation": {
      "base": "flex items-center gap-8 px-6 py-4 bg-[#212121] shadow-sm",
      "link": "text-[#d8d8d8] hover:text-[#3a4660] transition-colors duration-200 font-medium",
      "active": "text-[#3a4660] font-semibold"
    },
    "hero": {
      "base": "relative min-h-screen flex items-center justify-center bg-gradient-to-br from-[#3a4660] to-[#c9af98]",
      "overlay": "absolute inset-0 bg-black/40",
      "content": "relative z-10 text-center text-white space-y-6"
    }
  },
  "utilities": {
    "text": {
      "gradient": "bg-gradient-to-r from-[#3a4660] to-[#845007] bg-clip-text text-transparent",
      "shadow": "drop-shadow-lg",
      "truncate": "truncate",
      "ellipsis": "text-ellipsis overflow-hidden"
    },
    "animation": {
      "fadeIn": "animate-[fadeIn_300ms_ease-out]",
      "fadeOut": "animate-[fadeOut_300ms_ease-out]",
      "slideUp": "animate-[slideUp_400ms_ease-out]",
      "slideDown": "animate-[slideDown_400ms_ease-out]",
      "scaleIn": "animate-[scaleIn_200ms_ease-out]",
      "float": "animate-[float_3s_ease-in-out_infinite]",
      "spin": "animate-spin",
      "pulse": "animate-pulse",
      "bounce": "animate-bounce"
    },
    "glass": {
      "light": "bg-white/10 backdrop-blur-lg",
      "medium": "bg-white/20 backdrop-blur-xl",
      "dark": "bg-black/20 backdrop-blur-lg"
    }
  },
  "responsive": {
    "mobile": "@media (max-width: 640px)",
    "tablet": "@media (min-width: 641px) and (max-width: 1024px)",
    "desktop": "@media (min-width: 1025px)",
    "wide": "@media (min-width: 1920px)"
  }
}